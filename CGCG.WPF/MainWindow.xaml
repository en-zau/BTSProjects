<Window x:Class="CGCG.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CGCG.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
            <Button Content="Adherents" Click="Button_Click_adherent"/>
            <Button Content="Fournisseurs" Click="Button_Click_fournisseur"/>
        </StackPanel>
        <Frame x:Name="Main" Margin="0,35,0,0"/>


    </Grid>
</Window>
